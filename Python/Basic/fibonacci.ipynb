   "source": [
    "def fibonacci(n):\n",
    "    if(n==0):\n",
    "        return 0\n",
    "    elif(n==1):\n",
    "        return 1\n",
    "    else:\n",
    "        return fibonacci(n - 1) + fibonacci(n - 2)\n",
    "        \n",
    "\n",
    "\n",
    "n=int(input(\"Enter ending of series\"))\n",
    "for i in range(n):\n",
    "    print(fibonacci(i), end=\", \")"
   ]
