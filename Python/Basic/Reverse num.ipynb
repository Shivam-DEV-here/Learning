     "text": [
      "Enter the number 13\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Reversed num is 31\n"
     ]
    }
   ],
   "source": [
    "a=int(input(\"Enter the number\"))\n",
    "s=0\n",
    "while a!=0 :\n",
    "    x=a%10\n",
    "    s=s*10+x\n",
    "    a=a//10\n",
    "print(f\"Reversed num is {s}\")"
   ]
