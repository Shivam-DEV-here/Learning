   "source": [
    "def factorial(n):\n",
    "    if(n==1):\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial(n-1)\n",
    "\n",
    "\n",
    "\n",
    "n=int(input(\"Enter num\"))\n",
    "print(\"fact=\", factorial(n))"
